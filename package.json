{
  "name": "@schlenks/jest-progress-bar-reporter",
  "version": "2.0.0",
  "description": "Progress bar reporter for Jest (actively maintained fork)",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">=22"
  },
  "peerDependencies": {
    "jest": "^29.0.0 || ^30.0.0"
  },
  "scripts": {
    "jest": "jest",
    "test": "pnpm run jest && pnpm run check",
    "check": "biome check .",
    "fix": "biome check --write .",
    "example": "cd example/ && jest",
    "preversion": "pnpm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schlenks/jest-progress-bar-reporter.git"
  },
  "keywords": [
    "jest",
    "reporter",
    "progress",
    "progress-bar"
  ],
  "author": "schlenks (originally by Peio Roth)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schlenks/jest-progress-bar-reporter/issues"
  },
  "homepage": "https://github.com/schlenks/jest-progress-bar-reporter#readme",
  "dependencies": {
    "picocolors": "^1.1.1",
    "progress": "2.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "jest": "30.2.0"
  },
  "jest": {
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/example/"
    ]
  }
}
